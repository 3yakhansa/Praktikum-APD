            <output expression="&quot;Jika ia membeli beras Mawar ia akan membayar&quot; &amp; &quot; &quot; &amp; &quot;Rp.&quot; &amp; berasMawar &amp; &quot; &quot; &amp; &quot;setelah mendapat diskon 11%&quot;" newline="True"/>
            <output expression="&quot;Jika ia membeli beras Sania ia akan membayar&quot; &amp; &quot; &quot; &amp; &quot;Rp.&quot; &amp; berasSania &amp; &quot; &quot; &amp; &quot;setelah mendapat diskon 14%&quot;" newline="True"/>
